<div id="topHeaderArea">
    <div class="pageContent">
        <div class="row">
            <div class="col-md-4">
                {{#link-to "index" tagName="a"}}
                    <img src="/images/kodegenet_logo.png" style="height: 45px; float:left;" />
                {{/link-to}}
            </div>
            <div class="col-md-8">
                <ul>
                    {{#each page in pages}}
                        <li>
                        {{#link-to page.route tagName="a"}}
                            {{page.name}}
                        {{/link-to}}
                        </li>
                    {{/each}}
                </ul>
            </div>
        </div>
    </div>
</div>